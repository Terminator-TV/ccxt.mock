{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "class-name": false,
      "no-unnecessary-initializer": false,
      "object-literal-shorthand": false,
      "no-string-literal": false,
      "one-variable-per-declaration": false,
      "no-bitwise": false,
      "variable-name": {
        "options": [
          "ban-keywords",
          "check-format",
          "allow-pascal-case"
        ]
      }
    },
    "rulesDirectory": []
}